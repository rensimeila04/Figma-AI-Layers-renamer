{
    "name": "figma-ai-renamer",
    "version": "1.0.0",
    "description": "Figma plugin to rename layers using AI",
    "main": "dist/code.js",
    "scripts": {
        "build": "npm run build:ui && npm run build:code",
        "build:ui": "vite build",
        "build:code": "esbuild src/code.ts --bundle --outfile=dist/code.js --target=es2020",
        "dev": "vite build --watch",
        "typecheck": "tsc --noEmit"
    },
    "license": "ISC",
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@figma/plugin-typings": "^1.84.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "esbuild": "^0.27.2",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vite-plugin-singlefile": "^2.0.0"
    }
}